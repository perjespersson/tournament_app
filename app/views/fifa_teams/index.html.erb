<p id="notice"><%= notice %></p>

<h1>Fifa Teams</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Stars</th>
      <th>Career</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fifa_teams.each do |fifa_team| %>
      <tr>
        <td><%= fifa_team.name %></td>
        <td><%= fifa_team.stars %></td>
        <td><%= fifa_team.career %></td>
        <td><%= link_to 'Show', fifa_team %></td>
        <td><%= link_to 'Edit', edit_fifa_team_path(fifa_team) %></td>
        <td><%= link_to 'Destroy', fifa_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fifa Team', new_fifa_team_path %>
